<h1></h1>
<section class="card">
  <a href="/blogs/{{hid}}" class="card-body">
    <!-- The title of the blog post (will be set dynamically using Handlebars) -->
    <h3 class="card-title">{{title}}</h3>
    <h4>Posted by {{user.username}} on {{createdAt}}</h4>
    <p class="card-text">{{content}}</p>
  </a>
</section>

<!-- Script file for dashboard functionality -->
<script src="/js/dashboard.js"></script>